{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "rules": {
    "array-bracket-spacing": [2, "always"],
    "no-const-assign": 2,
    "no-var": "error",
    "indent": [2, 2],
    "quotes": [2, "backtick"],
    "eqeqeq": "error"
  }
}
